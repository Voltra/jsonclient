{
  "name": "@voltra/json",
  "version": "2.1.0",
  "description": "a JavaScript HTTP client specialized for JSON communications, based upon the Fetch API",
  "main": "build-babel/jsonclient.js",
  "scripts": {
    "setup": "npm i && npm update && npm update -D",
    "build": "npm run buildTsc && npm run buildBabel && npm run webpack",
    "buildTsc": "./node_modules/.bin/tsc",
    "buildBabel": "./node_modules/.bin/babel build-tsc --out-dir build-babel",
    "webpack": "./node_modules/.bin/webpack --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voltra/jsonclient.git"
  },
  "keywords": [
    "json",
    "client",
    "http",
    "https",
    "request",
    "simple",
    "promise",
    "fetch"
  ],
  "author": "Voltra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Voltra/jsonclient/issues"
  },
  "homepage": "https://github.com/Voltra/jsonclient#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "fetch_json": "^2.2.0"
  }
}
